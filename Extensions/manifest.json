{
    "name": "Unilogin Autocomplete",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Automatically fill out unilogin",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://broker.unilogin.dk/auth/realms/broker/*", "https://idp.unilogin.dk/auth/realms/idp/protocol/*"],
            "js": ["login.js"]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
     ]
}